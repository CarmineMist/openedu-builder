---
title: {{ title }}
{% if frontmatter %}
{% for key, value in frontmatter %}
{{ key }}: {{ value }}
{% endfor %}
{% endif %}
---

import useBaseUrl from '@docusaurus/useBaseUrl';

<div style={{ '{{' }}display: "flex", width: "100%", height: "100%", flexDirection: "row"{{ '}}' }}>
    <iframe style={{ '{{' }}flexGrow: 1, border: "none", margin: 0, padding: 0{{ '}}' }}, width="{{ width }}" height="{{ height }}" src={useBaseUrl('/{{ deck_location }}/index.html')}></iframe>
</div>
{% if link %}
<a href='{useBaseUrl('/{{ deck_location }}/index.html')}'>{{ link }}</a>
{% endif %}